@{
    ViewData["Title"] = "Execution";
}

<div class="bg-color-black slider closed">
    <div class="grid screen black-screen bg-color-black">
        <div class="image">
            <img src="~/images/fingerprint-green.svg" class="fingerprint green" alt="green fingerprint">
        </div>
    </div>
    <div tabindex="0" id="screen" style="display:none;" class="screen color-screen">
        <div class="image">
            <img src="~/images/fingerprint-black.svg" class="fingerprint black" alt="black fingerprint">
        </div>
    </div>
    <div class="player-container">
        <h3>PLAYER</h3>
        <!-- <button class="btn">button</button> -->
        <input id="player">
    </div>
    <div class="question-layout hidden">
        <div class="question-header">
            <h3>11 What year did the mole graduate from high school</h3>
        </div>
        <ul class="question-choices">
            <li><span>1</span> 1995</li>
            <li><span>2</span> 1955</li>
            <li><span>3</span> 1989</li>
            <li><span>4</span> 1963</li>
            <li><span>5</span> 1990</li>
            <li><span>6</span> 1998</li>
        </ul>
    </div>
</div>
<script src="~/lib/jquery/dist/jquery.js"></script>
<script src="~/js/mole.js"></script>